{"ast":null,"code":"const navigation = [{\n  name: 'Our Story',\n  href: '#'\n}, {\n  name: 'Schedule',\n  href: '#'\n}, {\n  name: 'Gallery',\n  href: '#'\n}, {\n  name: 'GIFT',\n  href: '#'\n}];\nconst mobileMenuOpen = ref(false);\n// @ is an alias to /src\nimport { ref } from 'vue';\nimport { Dialog, DialogPanel } from '@headlessui/vue';\nimport { Bars3Icon, XMarkIcon } from '@heroicons/vue/24/outline';\nimport Countdown from '../components/countdown.vue';\nexport default {\n  // eslint-disable-next-line vue/multi-word-component-names\n  name: 'LandingPage',\n  components: {\n    Dialog,\n    DialogPanel,\n    Bars3Icon,\n    XMarkIcon,\n    Countdown\n  },\n  data() {\n    return {\n      navigation,\n      mobileMenuOpen,\n      isScrolled: false,\n      desktopImage: require('../assets/lg.jpg'),\n      mobileImage: require('@/assets/28.jpg')\n    };\n  },\n  computed: {\n    currentImage() {\n      return window.innerWidth >= 767 ? `url('${this.desktopImage}')` : `url('${this.mobileImage}')`;\n    }\n  },\n  mounted() {\n    window.addEventListener('scroll', this.handleScroll);\n    window.addEventListener('resize', this.updateBackgroundImage);\n    this.updateBackgroundImage(); // Initial setup\n  },\n  beforeUnmount() {\n    window.removeEventListener('scroll', this.handleScroll);\n    window.removeEventListener('resize', this.updateBackgroundImage);\n  },\n  methods: {\n    handleScroll() {\n      this.isScrolled = window.scrollY > 0;\n    },\n    updateBackgroundImage() {\n      this.$forceUpdate(); // Force a re-render\n    },\n    getBackgroundImage() {\n      return window.innerWidth >= 767 ? `url('${this.desktopImage}')` : `url('${this.mobileImage}')`;\n    }\n  }\n};","map":{"version":3,"names":["navigation","name","href","mobileMenuOpen","ref","Dialog","DialogPanel","Bars3Icon","XMarkIcon","Countdown","components","data","isScrolled","desktopImage","require","mobileImage","computed","currentImage","window","innerWidth","mounted","addEventListener","h